{
	"workbench.settings.editor": "json",
	"editor.minimap.enabled": false,
	"breadcrumbs.enabled": true,
	"editor.fontSize": 17.3,
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": "active",

	"prettier.singleQuote": true,
	"prettier.useTabs": true,
	"workbench.colorCustomizations": {
		"editorCursor.foreground": "#fff",
		"editorCursor.opacity": "0.2",
		"editor.lineHighlightBackground": "#e3e3e312"
	},
	"editor.formatOnSaveMode": "modificationsIfAvailable",
	"eslint.format.enable": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},


	"editor.fontLigatures": true,
	"editor.fontFamily": "Anonymous Pro",
	"editor.detectIndentation": true,
	"terminal.integrated.fontSize": 16,
	"workbench.iconTheme": "helium-icon-theme",

	
	"workbench.colorTheme": "Seti Black",
	"editor.accessibilitySupport": "off",
	"helium-icon-theme.activeIconPack": "react",
	"helium-icon-theme.folders.color": "#90a4ae",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"explorer.confirmDragAndDrop": false,
	"explorer.compactFolders": false,
	"smartsemicolon.enable": false,
	"smartsemicolon.autoLineChange": false,
	"editor.formatOnSave": true,
	"files.defaultLanguage": "markdown",
	"window.zoomLevel": 1,
	"liveServer.settings.donotShowInfoMsg": true,
	"workbench.activityBar.visible": false
}
